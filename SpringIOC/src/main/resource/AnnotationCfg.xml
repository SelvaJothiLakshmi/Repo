<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns = "http://www.springframework.org/schema/beans"
   xmlns:xsi = "http://www.w3.org/2001/XMLSchema-instance"
   xmlns:context = "http://www.springframework.org/schema/context"
   xsi:schemaLocation = "http://www.springframework.org/schema/beans
   http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
   http://www.springframework.org/schema/context
   http://www.springframework.org/schema/context/spring-context-3.0.xsd">


<context:annotation-config/>

<bean id = "cust" class="SpringNewPrject.SpringNewPrject1.Custom" >
<property name="custmerId" value="1001"></property>
<property name="custName" value="custm1"></property>
</bean>

<bean id = "address" class="SpringNewPrject.SpringNewPrject1.Address" >
 <property name="doorNo" value="26"></property>
<property name="streetName" value="MystreetName"></property> 
</bean>

<bean id = "addressAnno" class="SpringNewPrject.SpringNewPrject1.Address" >
 <property name="doorNo" value="26/1"></property>
<property name="streetName" value="MystreetannQualifier"></property> 
</bean>

</beans>
